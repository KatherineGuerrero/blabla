<p id="notice"><%= notice %></p>

<h1>User Follow Playlists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_follow_playlists.each do |user_follow_playlist| %>
      <tr>
        <td><%= link_to 'Show', user_follow_playlist %></td>
        <td><%= link_to 'Edit', edit_user_follow_playlist_path(user_follow_playlist) %></td>
        <td><%= link_to 'Destroy', user_follow_playlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Follow Playlist', new_user_follow_playlist_path %>
